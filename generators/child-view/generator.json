{
  "files": [
    {
      "from": "view.js.hbs",
      "to": "app/views/{{name}}.js"
    },
    {
      "from": "index-requires.js.hbs",
      "to": "app/views/index.js",
      "method": "append"
    }
  ],
  "dependencies" : [
    {"type": "template", "parentPath": "{{parentPath}}/templates"},
    {"type": "style", "parentPath": "{{parentPath}}/styles"}
  ]
}
